{% extends 'base.html.twig' %}

{% block title %}Registrace{% endblock %}

{% block body %}
<div class="container mx-auto p-4 flex justify-center align-center">
    {{ form_start(form) }}
    {{ form_row(form.email) }}
    {{ form_row(form.password) }}
    <div class="mt-3">
        {{ form_widget(form.submit) }}
    </div>
    {{ form_end(form) }}
</div>
{% endblock %}